Public Class ClsIdNameCreatedBy
    Inherits ClsIdName

    Private sUserId As String = ""
    Private sDateOn As Date = DateDefault

    Public Property UserId() As String
        Get
            Return sUserId
        End Get
        Set(ByVal value As String)
            sUserId = value.Trim
        End Set
    End Property

    Public Property DateOn() As Date
        Get
            Return sDateOn
        End Get
        Set(ByVal value As Date)
            sDateOn = value
        End Set
    End Property

End Class
